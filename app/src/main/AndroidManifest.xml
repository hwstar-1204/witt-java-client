        </activity>
        <activity
            android:name=".Activity.CalenderSubActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".Activity.CalenderActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".Activity.CreateExercizeActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.CreateRoutineActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.ExercizeResultActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.MainActivity"
            android:exported="false"/>
